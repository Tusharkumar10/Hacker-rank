#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n;
    scanf("%d", &n);
    
    
    int sum=0;
    while(0<n)   // 10564
    {
        int a=n%10;  // 10564 % 10 = 4
        sum=sum+a;   // 4
        n=n/10;      // 1056
    }
    printf("%d",sum);
    return 0;
}
